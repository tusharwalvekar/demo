        <calculation><![CDATA[(function calculatedFieldValue(current) {
	var ratio = parseFloat(parseInt(current.cuthroats_won) / parseInt(current.cutthroats_lost));
	if (isNaN(ratio)) {
		return '0.00';
	}
	if (current.singles_lost == 0) {
		return parseInt(current.cuthroats_won).toFixed(2) + '';
	}
	return ratio.toFixed(2);
})(current);]]></calculation>
